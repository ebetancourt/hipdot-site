{
    "name": "hipdot-site",
    "version": "1.0.0",
    "description": "HipDot Media website â€” Eleventy 3.0+ with TypeScript/React SSR and Tailwind CSS.",
    "type": "module",
    "main": "tailwind.config.js",
    "scripts": {
        "clean": "rimraf _site",
        "dev": "npm-run-all clean --parallel dev:11ty dev:tailwind",
        "dev:11ty": "eleventy --serve --quiet --incremental",
        "dev:tailwind": "tailwindcss -i ./src/tailwind.css -o ./assets/css/style.css --watch",
        "build": "npm-run-all clean build:11ty build:tailwind",
        "build:11ty": "eleventy",
        "build:tailwind": "tailwindcss -i ./src/tailwind.css -o ./assets/css/style.css --minify",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
